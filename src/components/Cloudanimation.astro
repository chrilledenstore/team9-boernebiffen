---
import Cloudsm from "../../public/imgs/cloud-sm.svg";
import Cloud from "../../public/imgs/cloud.svg";
import Cloudmd from "../../public/imgs/cloud-md.svg";
import Cloudxl from "../../public/imgs/cloud-xl.svg";
---

<article class="overflow-hidden w-full h-full grid grid-cols-1 grid-rows-12 z-0">
  <Cloudmd width={200} height={100} fill="currentColor" class="grid col-start-1 row-start-1 animate-fromright" />
  <Cloud width={150} height={100} fill="currentColor" class="grid col-start-1 row-start-2 animate-fromleft" />
  <Cloud width={300} height={250} fill="currentColor" class="grid col-start-1 row-start-1 animate-fromleftslow" />
  <Cloudxl width={200} height={150} fill="currentColor" class="grid col-start-1 row-start-3 animate-fromrightslow" />
  <Cloudsm width={75} height={100} fill="currentColor" class="grid col-start-1 row-start-5 animate-fromleftfast" />
  <Cloudsm width={300} height={200} fill="currentColor" class="grid col-start-1 row-start-5 animate-fromleftslow" />
  <Cloudmd width={100} height={100} fill="currentColor" class="grid col-start-1 row-start-6 animate-fromright" />
  <Cloud width={200} height={100} fill="currentColor" class="grid col-start-1 row-start-7 animate-fromleft" />
</article>
